    "enabled": false,
    "options": ""
  },
  "cloudflare": {
    "enabled": false,
    "apiKey": "SECRETKEY",
    "email": "account@email.com",
    "domain": "domain-to-clear.com",
    "zoneId": "domain-specific-id"
  }
}
